# print the string characters: Mary had a little lamb.
print "Mary had a little lamb."

# print the string characters: Its fleece was white as snow
# Using format string
print "Its fleece was white as %s" % 'snow'

# print the string characters: And everywhere that Mary went.
print "And everywhere that Mary went."

print "." * 10 # will print '.' 10 times?

